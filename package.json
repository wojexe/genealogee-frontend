{
  "name": "genealogy-frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 vite dev",
    "build": "paraglide-js compile --project ./project.inlang && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "postinstall": "paraglide-js compile --project ./project.inlang",
    "biome:format": "biome format --write ./*.{js,json} ./src/{lib,routes} ./src/*.{html,ts,pcss} ./tests",
    "biome:lint": "biome lint --write ./*.{js,json} ./src/{lib,routes} ./src/*.{html,ts,pcss} ./tests",
    "biome:check": "biome check --write ./*.{js,json} ./src/{lib,routes} ./src/*.{html,ts,pcss} ./tests"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@inlang/paraglide-js": "1.11.2",
    "@inlang/paraglide-sveltekit": "^0.11.0",
    "@playwright/test": "1.45.3",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-vercel": "^5.3.0",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tanstack/svelte-query-devtools": "^5.29.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "svelte": "^5.0.0-next.94",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@internationalized/date": "^3.5.2",
    "@tanstack/svelte-query": "^5.28.4",
    "bits-ui": "^0.21.7",
    "clsx": "^2.1.0 ",
    "flag-icons": "^7.2.1",
    "formsnap": "^1.0.0",
    "lucide-svelte": "^0.373.0",
    "mode-watcher": "^0.3.0",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "sveltekit-superforms": "^2.12.2",
    "tailwind-merge": "^2.2.2",
    "tailwind-variants": "^0.2.1",
    "valibot": "^0.30.0"
  }
}
