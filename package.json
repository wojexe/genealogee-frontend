{
  "name": "genealogy-frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 vite dev",
    "preview": "NODE_TLS_REJECT_UNAUTHORIZED=0 vite preview",
    "build": "paraglide-js compile --project ./project.inlang && vite build",
    "build:staging": "paraglide-js compile --project ./project.inlang && vite build --mode staging",
    "clean:build": "rm -rf ./.svelte-kit ./.vercel",
    "clean:modules": "rm -rf ./node_modules",
    "clean:clean": "rm -rf ./test-results",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "postinstall": "paraglide-js compile --project ./project.inlang",
    "biome:format": "biome format --write ./*.{js,json} ./src/{lib,routes} ./src/*.{html,ts,pcss} ./tests",
    "biome:lint": "biome lint --write ./*.{js,json} ./src/{lib,routes} ./src/*.{html,ts,pcss} ./tests",
    "biome:check": "biome check --write ./*.{js,json} ./src/{lib,routes} ./src/*.{html,ts,pcss} ./tests"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@inlang/paraglide-js": "1.11.2",
    "@inlang/paraglide-sveltekit": "^0.11.0",
    "@playwright/test": "1.45.3",
    "@sveltejs/adapter-vercel": "5.4.3",
    "@sveltejs/kit": "2.5.24",
    "@sveltejs/vite-plugin-svelte": "4.0.0-next.6",
    "@tanstack/svelte-query-devtools": "^5.29.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "postcss-load-config": "^6.0.1",
    "svelte": "^5.0.0-next.238",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "2.0.5"
  },
  "dependencies": {
    "@tanstack/svelte-query": "https://pkg.pr.new/@tanstack/svelte-query@0cd7069",
    "bits-ui": "^0.21.13",
    "clsx": "^2.1.1",
    "flag-icons": "^7.2.3",
    "formsnap": "^1.0.1",
    "lucide-svelte": "^0.435.0",
    "mode-watcher": "0.4.1",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "sveltekit-superforms": "2.17.0",
    "tailwind-merge": "^2.5.2",
    "tailwind-variants": "^0.2.1",
    "valibot": "0.39.0"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@sveltejs/kit",
    "esbuild",
    "svelte-preprocess"
  ]
}
